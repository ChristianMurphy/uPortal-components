{
  "name": "content-grid",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "buildold": "vue-cli-service build --name content-grid --target wc 'src/components/ContentGrid.vue'",
    "build": "vue-cli-service build --name content-grid --target wc 'src/mainwc.js'",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@uportal/open-id-connect": "^1.2.0",
    "portlet-card": "^0.1.7",
    "vue": "^2.5.16",
    "vue-i18n": "^7.8.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.3",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.3",
    "@vue/cli-service": "^3.0.0-rc.3",
    "@vue/eslint-config-prettier": "^3.0.0-rc.3",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "vue-template-compiler": "^2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/content-grid.js",
  "description": "Display all browsable uPortal portlet as cards",
  "license": "Apache-2.0",
  "author": "Julien Gribonvald <julien.gribonvald@gmail.com>",
  "keywords": [
    "apereo",
    "uportal",
    "portlet",
    "grid"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgribonvald/uPortal-web-components.git"
  },
  "bugs": {
    "url": "https://github.com/jgribonvald/uPortal-web-components/issues"
  },
  "homepage": "https://github.com/jgribonvald/uPortal-web-components#readme"
}
