{
  "name": "esco-content-menu",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target wc --name esco-content-menu './src/mainwc.js'",
    "buildold": "vue-cli-service build --name esco-content-menu --target wc 'src/components/EscoContentMenu.vue'",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@gip-recia/content-grid": "^0.1.1",
    "@uportal/content-carousel": "^1.2.0",
    "@uportal/open-id-connect": "^1.2.0",
    "@vue/web-component-wrapper": "^1.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "vue": "^2.5.16",
    "vue-awesome": "^2.3.7",
    "vue-awesome-swiper": "^3.1.3",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.3",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.3",
    "@vue/cli-service": "^3.0.0-rc.3",
    "vue-template-compiler": "^2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
